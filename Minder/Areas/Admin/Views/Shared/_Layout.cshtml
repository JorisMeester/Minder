
<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Gentelella Alela! | </title>
    <!-- Bootstrap -->
    @Styles.Render("~/Areas/Admin/vendors/bootstrap/dist/css/bootstrap.min.css")
    <!-- Font Awesome -->
    @Styles.Render("~/Areas/Admin/vendors/font-awesome/css/font-awesome.min.css")
    <!-- NProgress -->
    @Styles.Render("~/Areas/Admin/vendors/nprogress/nprogress.css")
    <!-- iCheck -->
    @Styles.Render("~/Areas/Admin/vendors/iCheck/skins/flat/green.css")

    <!-- bootstrap-progressbar -->
    @Styles.Render("~/Areas/Admin/vendors/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css")
    <!-- JQVMap -->
    @Styles.Render("~/Areas/Admin/vendors/jqvmap/dist/jqvmap.min.css")
    <!-- bootstrap-daterangepicker -->
    @Styles.Render("~/Areas/Admin/vendors/bootstrap-daterangepicker/daterangepicker.css")
    <!-- Custom Theme Style -->
    @Styles.Render("~/Areas/Admin/build/css/custom.min.css")
</head>
<body class="nav-md">
    <div class="container body">
        <div class="main_container">
            <div class="col-md-3 left_col">
                <div class="left_col scroll-view">
                    <div class="navbar nav_title" style="border: 0;">
                        <a href="index.html" class="site_title"><i class="fa fa-paw"></i> <span>Gentelella Alela!</span></a>
                    </div>
                    <div class="clearfix"></div>
                    <!-- /menu profile quick info -->
                    <br />
                    <!-- sidebar menu -->
                    <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
                        <div class="menu_section">
                            <h3>Beheer</h3>
                            <ul class="nav side-menu">
                                <li>
                                <li>@Html.ActionLink("Berichten", "index", "#") </li>
                                <li>
                            </ul>
                        </div>
                    </div>
                </div>
                <!-- /sidebar menu -->
            </div>
        </div>
        <!-- top navigation -->
        <div class="top_nav">
            <div class="nav_menu">
                <nav>
                    <div class="nav toggle">
                        <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                    </div>
                </nav>
            </div>
        </div>
        <!-- /top navigation -->
        <!-- page content -->
        <div class="right_col" role="main">
            @RenderBody()
            <div class="col-md-8 col-sm-8 col-xs-12">

                <div class="row">
                    <div class="col-md-12 col-sm-12 col-xs-12">
                        <!-- end of weather widget -->
                    </div>
                </div>
            </div>
        </div>
        <!-- /page content -->
        <!-- footer content -->
        <footer>
            <div class="pull-right">
                Minder
            </div>
            <div class="clearfix"></div>
        </footer>
        <!-- /footer content -->
        <!-- jQuery -->
        @Scripts.Render("~/Areas/Admin/vendors/jquery/dist/jquery.min.js")
        <!-- Bootstrap -->
        @Scripts.Render("~/Areas/Admin/vendors/bootstrap/dist/js/bootstrap.min.js")
        <!-- FastClick -->
        @Scripts.Render("~/Areas/Admin/vendors/fastclick/lib/fastclick.js")
        <!-- NProgress -->
        @Scripts.Render("~/Areas/Admin/vendors/nprogress/nprogress.js")
        <!-- Chart.js -->
        @Scripts.Render("~/Areas/Admin/vendors/Chart.js/dist/Chart.min.js")
        <!-- gauge.js -->
        @Scripts.Render("~/Areas/Admin/vendors/gauge.js/dist/gauge.min.js")
        <!-- bootstrap-progressbar -->
        @Scripts.Render("~/Areas/Admin/vendors/bootstrap-progressbar/bootstrap-progressbar.min.js")
        <!-- iCheck -->
        @Scripts.Render("~/Areas/Admin/vendors/iCheck/icheck.min.js")
        <!-- Skycons -->
        @Scripts.Render("~/Areas/Admin/vendors/skycons/skycons.js")
        <!-- Flot -->
        @Scripts.Render("~/Areas/Admin/vendors/Flot/jquery.flot.js")
        @Scripts.Render("~/Areas/Admin/vendors/Flot/jquery.flot.pie.js")
        @Scripts.Render("~/Areas/Admin/vendors/Flot/jquery.flot.time.js")
        @Scripts.Render("~/Areas/Admin/vendors/Flot/jquery.flot.stack.js")
        @Scripts.Render("~/Areas/Admin/vendors/Flot/jquery.flot.resize.js")
        <!-- Flot plugins -->
        @Scripts.Render("~/Areas/Admin/vendors/flot.orderbars/js/jquery.flot.orderBars.js")
        @Scripts.Render("~/Areas/Admin/vendors/flot-spline/js/jquery.flot.spline.min.js")
        @Scripts.Render("~/Areas/Admin/vendors/flot.curvedlines/curvedLines.js")
        <!-- DateJS -->
        @Scripts.Render("~/Areas/Admin/vendors/DateJS/build/date.js")
        <!-- JQVMap -->
        @Scripts.Render("~/Areas/Admin/vendors/jqvmap/dist/jquery.vmap.js")
        @Scripts.Render("~/Areas/Admin/vendors/jqvmap/dist/maps/jquery.vmap.world.js")
        @Scripts.Render("~/Areas/Admin/vendors/jqvmap/examples/js/jquery.vmap.sampledata.js")
        <!-- bootstrap-daterangepicker -->
        @Scripts.Render("~/Areas/Admin/vendors/moment/min/moment.min.js")
        @Scripts.Render("~/Areas/Admin/vendors/bootstrap-daterangepicker/daterangepicker.js")
        <!-- Custom Theme Scripts -->
        @Scripts.Render("~/Areas/Admin/build/js/custom.min.js")

</body>
</html>
